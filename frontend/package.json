{
  "name": "prosjekt3redux",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://it2810-42.idi.ntnu.no/prosjekt4/",
  "dependencies": {
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.5",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "fix": "eslint --fix src/**/*.js",
    "cypress:open": "cypress open"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "node": true,
      "es6": true
    },
    "extends": [
      "react-app",
      "eslint:recommended"
    ],
    "rules": {
      "indent": [
        "error",
        4
      ],
      "no-console": 0,
      "no-var": "error",
      "curly": "error",
      "no-multi-spaces": "error",
      "array-bracket-spacing": "error",
      "brace-style": "error",
      "camelcase": "error",
      "comma-spacing": [
        "error",
        {
          "before": false,
          "after": true
        }
      ],
      "comma-style": [
        "error",
        "last"
      ],
      "func-call-spacing": [
        "error",
        "never"
      ],
      "keyword-spacing": "error",
      "no-multiple-empty-lines": "error",
      "no-trailing-spaces": "error",
      "quotes": [
        "error",
        "single"
      ],
      "semi-style": [
        "error",
        "last"
      ],
      "space-in-parens": [
        "error",
        "always"
      ],
      "prefer-const": "error",
      "quote-props": "error",
      "space-before-blocks": "error",
      "space-infix-ops": "error",
      "newline-per-chained-call": "error",
      "semi": [
        "error",
        "always"
      ],
      "no-unreachable": "error",
      "no-unused-vars": "warn",
      "no-mixed-spaces-and-tabs": 0
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "cypress": "^3.1.0",
    "redux-devtools": "^3.4.1"
  }
}
